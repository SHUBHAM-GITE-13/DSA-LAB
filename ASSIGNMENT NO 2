def linear_search(arr, target):
    for item in arr:
        if item == target:
            return True
    return False


def binary_search(arr, target):
    low = 0
    high = len(arr) - 1

    while low <= high:
        mid = (low + high) // 2
        if arr[mid] == target:
            return True
        elif arr[mid] < target:
            low = mid + 1
        else:
            high = mid - 1

    return False


customer_ids = [102, 101, 205, 150, 300, 250]

search_id = 150

print("Linear Search Result:", linear_search(customer_ids, search_id))

sorted_customer_ids = sorted(customer_ids)
print("Binary Search Result:", binary_search(sorted_customer_ids, search_id))
